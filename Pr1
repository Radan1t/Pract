#include <stdio.h>
int main() {
    int t1, t2, t3;
    double sumt,endt;
    printf("Inpute time 3 times:\n");
    // Введення часу
    scanf("%d %d %d",&t1,&t2,&t3);
    //Перевірка умови
    if (t1>10000||t2>10000||t3>10000)
    {
        printf("Wrong numbers");
        return;
    }
    else{ 
    // Обчислення загального часу
    sumt=1.0/t1+1.0/t2+1.0/t3;
    // Обчислення часу кінцевого
    endt=1.0/sumt;
    // Виведення результату
    printf("Time that need for all cake:%.2f\n", endt);
    }
    return 0;}
